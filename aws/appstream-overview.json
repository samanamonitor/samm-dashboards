          "editorMode": "code",
          "exemplar": false,
          "expr": "sum by (fqdn,role,layer) ((1-rate(cim_perfcpu_percentidletime{name=\"_Total\",role=\"aws_appstream\"}[$__rate_interval]) / rate(cim_perfcpu_timestamp_sys100ns {name=\"_Total\"}[$__rate_interval]))*100 > $cpu_threshold)",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "CPU"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "sum by (fqdn,role,layer) ((1-(cim_os_freephysicalmemory{role=\"aws_appstream\"} / cim_os_totalvisiblememorysize))*100 > $ram_threshold)",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "RAM"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "(max by (fqdn,role,layer) (1 - cim_disk_freespace{drivetype=\"3\",role=\"aws_appstream\"} / cim_disk_size{drivetype=\"3\"})*100 > $disk_threshold)",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "Disk"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "sum by (fqdn,address) (up{role=\"aws_appstream\"})\r\n+ on(address) group_left(userid,fleetname,stackname) (\r\n  0 * label_replace(\r\n    aws_appstream_sessions_state{userid=~\"$userid\",stackname=\"$stackname\",fleetname=\"$fleetname\"},\r\n    \"address\", \"$1\", \"networkaccessconfiguration_eniprivateipaddress\", \"(.*)\"\r\n  )\r\n)",
