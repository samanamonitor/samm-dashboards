      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "sum by (fqdn) ((1-rate(cim_perfcpu_percentidletime{name=\"_Total\",fqdn=~\"$fqdn\"}[$__rate_interval]) / rate(cim_perfcpu_timestamp_sys100ns {name=\"_Total\"}[$__rate_interval]))*100 > $cpu_threshold) + on (fqdn) group_left (desktopgroup_name) (label_replace(sum (ctx_machine_isinmaintenancemode{desktopgroup_name=\"$desktopgroup_name\"} * 0) without (hostingservername), \"fqdn\", \"$1\", \"dnsname\", \"(.*)\")) or clamp_max(absent(nonexistent{fqdn=\"none\"}), 0)",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "CPU"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "sum by (fqdn) ((1-(cim_os_freephysicalmemory / cim_os_totalvisiblememorysize))*100 > $ram_threshold) + on (fqdn) group_left (desktopgroup_name) (label_replace(sum (ctx_machine_isinmaintenancemode{desktopgroup_name=\"$desktopgroup_name\"}) without (hostingservername) * 0, \"fqdn\", \"$1\", \"dnsname\", \"(.*)\")) or clamp_max(absent(nonexistent{fqdn=\"none\"}), 0)",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "RAM"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "(max by (fqdn) (1 - cim_disk_freespace{drivetype=\"3\"} / cim_disk_size{drivetype=\"3\"})*100 > $disk_threshold) + on (fqdn) group_left (desktopgroup_name) (label_replace(sum (ctx_machine_isinmaintenancemode{desktopgroup_name=\"$desktopgroup_name\"}) without (hostingservername) * 0, \"fqdn\", \"$1\", \"dnsname\", \"(.*)\")) or clamp_max(absent(nonexistent{fqdn=\"none\"}), 0)",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "Disk"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "sum by (fqdn) (time() - cim_os_lastbootuptime) + on (fqdn) group_left (desktopgroup_name) (label_replace(sum (ctx_machine_isinmaintenancemode{desktopgroup_name=\"$desktopgroup_name\"}) without (hostingservername) * 0, \"fqdn\", \"$1\", \"dnsname\", \"(.*)\"))",
          "format": "table",
          "hide": false,
          "instant": true,
          "legendFormat": "__auto",
          "range": false,
          "refId": "Uptime"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "label_replace(sum by (dnsname) (ctx_machine_currentregistrationstate{desktopgroup_name=\"$desktopgroup_name\"}), \"fqdn\", \"$1\", \"dnsname\", \"(.*)\") $register_filter",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "Registration"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "c3a5e98c-6e94-44b8-b894-ae05241051b7"
          },
          "editorMode": "code",
          "exemplar": false,
          "expr": "label_replace(sum by (dnsname) (ctx_machine_currentpowerstate{desktopgroup_name=\"$desktopgroup_name\"}), \"fqdn\", \"$1\", \"dnsname\", \"(.*)\") $power_filter",
          "format": "table",
          "hide": false,
          "instant": true,
          "interval": "90s",
          "legendFormat": "__auto",
          "range": false,
          "refId": "Power"
        }
